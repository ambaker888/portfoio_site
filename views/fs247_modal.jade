#myModalfs.modal.fade(tabindex='-1', role='dialog', aria-labelledby='myModalLabel', aria-hidden='true')
  .modal-dialog
    .modal-content
      .modal-header
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') 
            &times;        
        h3.modal-title
         | FS 24/7 Ltd
        h4.subtitle.text-center
          | Web design and development    
      .modal-body
        .container
          .col-sm-4
            img.img-responsive(src='/images/home-fs.png', alt='') 
          .col-sm-4
            img.img-responsive(src='/images/fs-about-us.png', alt='') 
          .col-sm-4
            img.img-responsive(src='/images/services-fs.png', alt='') 
             
      //.modal-footer
        .col-xs-6
          a(href='fs247_modal.jade')
            button.btn.btn-primary(type='button') Next
              span.glyphicon.glyphicon-chevron-right
        .col-xs-6
          a(href='workorder_modal.jade')
            button.btn.btn-default(type='button', data-dismiss='modal') 
              span.glyphicon.glyphicon-chevron-left    Previous     
