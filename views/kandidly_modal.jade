#myModalkandidly.modal.fade(tabindex='-1', role='dialog', aria-labelledby='myModalLabel', aria-hidden='true')
  .modal-dialog
    .modal-content
      .modal-header
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') 
            &times;        
        h3.modal-title#myModalLabel 
         | Kandid.ly  
      .modal-body
        .container
          .col-sm-4
            img.img-responsive(src='/images/kandidnly-homepage.png', alt='') 
          .col-sm-4
            img.img-responsive(src='/images/kandidly-about.png', alt='') 
          .col-sm-4
            img.img-responsive(src='/images/kandidly-how-it-workscustomer.png', alt='') 
             
      .modal-footer
        .col-xs-12.text-center
          a(href='workorder_modal.jade')
            button.btn.btn-primary(type='button', data-dismiss='modal') 
              span.glyphicon.glyphicon-chevron-left    Previous  
          a(href='fs247_modal.jade')
            button.btn.btn-primary(type='button') Next
              span.glyphicon.glyphicon-chevron-right
  
            
          